<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="collier.mckennon.com.wguhoot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TermsActivity" />
        <activity android:name=".CoursesActivity" />
        <activity android:name=".AssessmentsActivity" />
        <activity android:name=".MentorsActivity" />
        <activity android:name=".AddAssessmentActivity" />
        <activity android:name=".AddCourseActivity" />
        <activity android:name=".AddMentorActivity" />
        <activity android:name=".AddTermActivity"></activity>
    </application>

</manifest>