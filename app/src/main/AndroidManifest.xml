<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="collier.mckennon.com.wguhoot">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:theme">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="DATABASE"
            android:value="hoot.db" />
        <meta-data
            android:name="VERSION"
            android:value="10" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="collier.mckennon.com.wguhoot" />

        <activity android:name=".Activity.term.TermsActivity" />
        <activity android:name=".Activity.course.CoursesActivity" />
        <activity android:name=".Activity.assessment.AssessmentsActivity" />
        <activity android:name=".Activity.mentor.MentorsActivity" />
        <activity android:name=".Activity.assessment.AddAssessmentActivity" />
        <activity android:name=".Activity.course.AddCourseActivity" />
        <activity android:name=".Activity.mentor.AddMentorActivity" />
        <activity android:name=".Activity.term.AddTermActivity" />
    </application>

</manifest>